import java.util.Scanner;
public class GCD {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int n =sc.nextInt();
		
		int divisor;
		int divident;
		int rem;
		
		while( divident/divisor != 0)
		{
			  rem=divident % divisor;
			  divident=divisor;
			  divisor=rem;
			  
		}
		
	}

}
