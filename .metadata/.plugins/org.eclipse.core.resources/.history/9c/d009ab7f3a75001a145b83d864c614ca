package Lecture1;

import java.util.*;

public class Patern1{

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		
		int n=sc.nextInt();
		int col=n;
		for(int row=0;row<=n;row++)
		{
			for(int col=0;col<=n;col++ )
			//work
			System.out.print("*");
			
			//preparation
			System.out.println();
		}
		
	}

}
