package Lecture1;

import java.util.*;

public class Patern1{

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		
		int n=sc.nextInt();
		
		for(int row=0;row<=n;row++)
		{
			for(row=0;row<=n;row++ )
			{
			//work
			System.out.println("*");
			
			}
		}
		
	}

}
