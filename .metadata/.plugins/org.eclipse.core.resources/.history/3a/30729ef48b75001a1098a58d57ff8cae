package Lecture1;
import java.util.*;

public class Pattern0While {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		
		int n=sc.nextInt();
		int row=0;
		
		while(row<=n)
		{
			//work
			System.out.print("*");
			
			//preparation
			row++;
			System.out.println();

		}
		
		
		
	}

}
