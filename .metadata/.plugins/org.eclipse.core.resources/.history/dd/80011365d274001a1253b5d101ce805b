import java.util.Scanner;

public class Prime {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		@SuppressWarnings("resource")
		Scanner sc=new Scanner(System.in);
		
		//boolean flag;
		int div=2;
		
		int n=sc.nextInt();
		
		while(div<=n/2)
		{
			
		div=div+1;
		}
	}

}
